{
  "name": "perguntas_respostas",
  "version": "1.0.0",
  "description": "Estudo de caso com NodeJS",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "dev": "npm run development && npm run start",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/mvanucci/perguntas_respostas.git"
  },
  "keywords": [],
  "author": "Murilo Vanucci",
  "license": "ISC",
  "homepage": "https://bitbucket.org/mvanucci/perguntas_respostas#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "mysql2": "^3.7.0",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "bootstrap": "^5.3.2",
    "cross-env": "^7.0.3",
    "jquery": "^3.7.1",
    "laravel-mix": "^6.0.49",
    "lodash": "^4.17.21",
    "popper.js": "^1.16.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.69.7",
    "sass-loader": "^14.0.0",
    "vue-template-compiler": "^2.7.16"
  }
}
